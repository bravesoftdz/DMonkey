function fac(n){
  return n <= 0 ? 1 : n * fac(n-1);
}

function factrial( n ){
    if( n <= 0 ){
        return 1 ;
    }else{
        return n*arguments.callee( n-1 );
    }
}


println('fac: ' + fac(10));
println('fac2: ' + factrial(10));

function createFunction( n ){
    return function( i ){
        if( i > 0 ){
            return Math.pow( i, n )+arguments.callee( i-1 );
        }else{
            return 0 ;
        }
    };
}

var sum2 = createFunction( 2 );
var sum4 = createFunction( 4 );

println( sum2( 5 ) ); // Œ‹‰Ê‚Í 55 = 1^2+2^2+3^2+4^2+5^2 = 1+4+9+16+25
println( sum4( 3 ) ); // Œ‹‰Ê‚Í 98 = 1^4+2^4+3^4 = 1+16+81



